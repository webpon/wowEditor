{
  "name": "wowEditor",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "webpon <2249096563@qq.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "win32": {
    "command": "wowEditor.exe"
  },
  "darwin": {
    "command": "open"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "less": "^4.1.3",
    "picgo": "^1.5.0",
    "typescript": "^5.0.3",
    "vite": "^4.0.3",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.16"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.1.7",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.4.1",
    "flowchart.js": "^1.17.1",
    "katex": "^0.16.4",
    "mermaid": "^9.3.0",
    "pinia": "^2.0.28",
    "tdesign-icons-vue-next": "^0.1.7",
    "tdesign-vue-next": "^1.0.2",
    "viz.js": "^2.1.2",
    "vue-router": "^4.1.6"
  }
}
